D,EAAqB;AAAA,6BAAiC,MAAA,CAAA;AAAjC,KAArB;AA2BJ,GA3BN,E,CAEa,MAAA,CAAA,MAAA,CAAO,MAAP,KAAa,C,kBADxB,YAAA,CAGM,KAHN,EAGM;UAAA;AAHD,IAAA,KAAK,EAAC,wBAGL;AADA,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,UAAA,IAAA,QAAA,CAAA,UAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AACL,GAHN,C,uCAKW,MAAA,CAAA,MAAA,CAAO,MAAP,KAAa,C,kBADxB,YAAA,CAOM,KAPN,EAOM;UAAA;AAPD,IAAA,KAAK,EAAA,CAAC,cAAD,EAAe;AAAA,kBAEC,MAAA,CAAA,YAAA,IAAgB,MAAA,CAAA;AAFjB,KAAf,CAOJ;AAJA,IAAA,KAAK,EAAE,QAAA,CAAA,KAIP;AAHA,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,UAAA,IAAA,QAAA,CAAA,UAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAGL,GAPN,E,oBAKE,YAAA,CACkE,SADlE,EACkE,IADlE,EACkE,WAAA,CADvC,QAAA,CAAA,KACuC,EADlC,UAAV,CAAU,EAAT;yBAAvB,YAAA,CACkE,wBADlE,EACkE;AAD/B,MAAA,GAAG,EAAE,CAC0B;AADtB,MAAA,KAAK,EAAE,CAAC,CAAA,CAAA,CACc;AADR,MAAA,KAAK,EAAE,CAAC,CAAA,CAAA,CACA;AAAlD,MAAA,WAAW,EAAE,QAAA,CAAA;AAAqC,KADlE,E,IAAA,E;;AAAA,M,iCAAA,C;GACkE,CADlE,E;;AAAA,G,EALF,E;;AAAA,G,uCAQA,YAAA,CAOmB,gBAPnB,EAOmB;AAPD,IAAA,IAAI,EAAC;AAOJ,GAPnB,EAA6B;qBAC3B;AAAA,aAKM,CAJK,KAAA,CAAA,W,kBADX,YAAA,CAKM,KALN,EAKM;cAAA;AALD,QAAA,KAAK,EAAA,CAAC,kBAAD,EAAmB;AAAA,sBAEH,MAAA,CAAA;AAFG,SAAnB,CAKJ;AAFA,QAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,iBAAE,QAAA,CAAA,cAAA,IAAA,QAAA,CAAA,cAAA,OAAA,QAAA,YAAF;AAAA,SAAA;AAEL,OALN,E,CAIE,U,CAJF,E;;AAAA,O,qCAKM,CALN;AAAA,K,CAD2B;;;;AAAA,GAA7B,C,EAUW,MAAA,CAAA,iBAAA,IAAiB,CAAK,KAAA,CAAA,W,kBADjC,YAAA,CAIM,KAJN,EAIM;UAAA;AAJD,IAAA,KAAK,EAAC,4BAIL;AAFA,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,mBAAA,IAAA,QAAA,CAAA,mBAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAEL,GAJN,E,CAGE,U,CAHF,C,sCAtBF,E;;AAAA,G","sourcesContent":["<template>\r\n  <div class=\"grid-block_wrapper\" :class=\"{'available-pawn-move': availablePawnMove}\">\r\n    <div class=\"move-pawn-here_wrapper\"\r\n         v-if=\"shapes.length === 0\"\r\n         @click=\"selectPawn\">\r\n    </div>\r\n    <div class=\"pawn_wrapper\"\r\n         v-if=\"shapes.length !== 0\"\r\n         :class=\"{'selected': selectedPawn && you}\"\r\n         :style=\"style\"\r\n         @click=\"selectPawn\">\r\n      <GridBlockPawn v-for=\"i in pawns\" :key=\"i\" :shape=\"i[0]\" :color=\"i[1]\"\r\n                     :customWidth=\"pawnPercentageWidth\"></GridBlockPawn>\r\n    </div>\r\n    <transition-group name=\"fade\">\r\n      <div class=\"obstacle_wrapper\"\r\n           v-if=\"hasObstacle\"\r\n           :class=\"{'selected': selectedObstacle}\"\r\n           @click=\"selectObstacle\">\r\n        <img src=\"@/assets/obstacle.png\" alt=\"\">\r\n      </div>\r\n    </transition-group>\r\n\r\n    <div class=\"move-obstacle-here_wrapper\"\r\n         v-if=\"obstacleAvailable && !hasObstacle\"\r\n         @click=\"clickedObstacleMove\">\r\n      <div class=\"move-obstacle-here\"></div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport GridBlockPawn from \"./GridBlockPawn\";\r\nimport websocketEvents from \"../../../constants/websocketEvents\";\r\n\r\nexport default {\r\n  name: \"GameGridBlock\",\r\n  components: {GridBlockPawn},\r\n  data(){\r\n    return {\r\n      hasObstacle: false\r\n    }\r\n  },\r\n  props: {\r\n    column: Number,\r\n    row: Number,\r\n    shapes: Array,\r\n    colors: Array,\r\n    you: Boolean,\r\n    obstacle: Boolean,\r\n    selectedPawn: Boolean,\r\n    selectedObstacle: Boolean,\r\n    availablePawnMove: Boolean,\r\n    obstacleAvailable: Boolean,\r\n    socket: Object\r\n  },\r\n  computed: {\r\n    pawns: function () {\r\n      let p = []\r\n      for (let i = 0; i < this.shapes.length; i++) {\r\n        p.push([this.shapes[i], this.colors[i]])\r\n      }\r\n      return p\r\n    },\r\n    pawnPercentageWidth: function () {\r\n      return 100 / this.pawns.length\r\n    },\r\n    style: function () {\r\n      let cols = this.colors.length === 1 ? 1 : 2\r\n      return {\r\n        'grid-template-columns': 'repeat(' + cols + ',' + 100 / cols + '%)',\r\n        'grid-template-rows': 'repeat(3, 33%)'\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    selectPawn: function () {\r\n      if (this.you) {\r\n        this.$emit(\"selectPawn\", [this.row, this.column])\r\n      } else {\r\n        this.clickedPawnMove()\r\n      }\r\n    },\r\n    selectObstacle: function () {\r\n      this.$emit(\"selectObstacle\", [this.row, this.column])\r\n    },\r\n    clickedPawnMove: function () {\r\n      if (this.availablePawnMove) {\r\n        this.$emit(\"movePawn\", [this.row, this.column])\r\n      }\r\n    },\r\n    clickedObstacleMove: function () {\r\n      this.$emit(\"moveObstacle\", [this.row, this.column])\r\n    },\r\n  },\r\n  mounted() {\r\n    this.hasObstacle = this.obstacle\r\n    this.socket.on(websocketEvents.MOVE_BLOCK, data => {\r\n      if (this.row === data.from_row && this.column === data.from_column){\r\n        this.hasObstacle = false\r\n      }else if(this.row === data.to_row && this.column === data.to_column){\r\n        this.hasObstacle = true\r\n      }\r\n    })\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.grid-block_wrapper {\r\n  position: relative;\r\n  width: 100%;\r\n  height: 100%;\r\n  border: 1px solid black;\r\n  //border-left: 1px solid black;\r\n  //border-right: 1px solid black;\r\n  .move-pawn-here_wrapper{\r\n    width: 100%;\r\n    height: 80%;\r\n  }\r\n  .pawn_wrapper {\r\n    width: 60%;\r\n    height: 80%;\r\n    margin-top: 10%;\r\n    margin-left: 20%;\r\n    display: grid;\r\n\r\n    &.selected {\r\n      //border: 1px solid red;\r\n    }\r\n  }\r\n\r\n  .obstacle_wrapper {\r\n    width: 90%;\r\n    margin-left: 5%;\r\n    height: fit-content;\r\n    position: absolute;\r\n    bottom: 0;\r\n\r\n    img {\r\n      width: 100%;\r\n    }\r\n  }\r\n\r\n  &.available-pawn-move {\r\n    background-color: yellow;\r\n\r\n    .pawn_wrapper {\r\n      opacity: .2;\r\n    }\r\n  }\r\n\r\n  .move-obstacle-here_wrapper {\r\n    position: absolute;\r\n    bottom: 0;\r\n    width: 100%;\r\n    height: 30%;\r\n    cursor: pointer;\r\n\r\n    .move-obstacle-here {\r\n      width: 80%;\r\n      height: 70%;\r\n      margin: auto;\r\n      border: 2px dotted red;\r\n      border-spacing: 5px;\r\n    }\r\n  }\r\n}\r\n\r\n.fade-enter-active, .fade-leave-active {\r\n  transition: opacity .5s ease;\r\n}\r\n\r\n.fade-enter-from, .fade-leave-to {\r\n  opacity: 0;\r\n}\r\n</style>"],"sourceRoot":""}]}