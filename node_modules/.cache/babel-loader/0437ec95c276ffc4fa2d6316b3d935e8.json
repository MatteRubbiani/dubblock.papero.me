div>\r\n</template>\r\n\r\n<script>\r\nimport Players from \"../gameSceneComponents/MenuComponents/Players\";\r\nimport PlayingPlayer from \"./MenuComponents/PlayingPlayer\";\r\nimport websocketEvents from \"../../../constants/websocketEvents\";\r\n\r\nexport default {\r\n  name: \"Menu\",\r\n  components: {PlayingPlayer, Players},\r\n  props: {\r\n    game: Object,\r\n    socket: Object\r\n  },\r\n  computed: {\r\n    playingPlayer: function () {\r\n      for (let i = 0; i < this.game.players.length; i++) {\r\n        if (this.game.players[i].playing) return this.game.players[i]\r\n      }\r\n      return null\r\n    }\r\n  },\r\n  methods: {\r\n    earthquake: function() {\r\n      this.socket.emit(websocketEvents.EARTHQUAKE, \"\")\r\n    },\r\n    revelation: function (){\r\n      this.socket.emit(websocketEvents.REVELATION, \"\")\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.menu_wrapper {\r\n  height: 100%;\r\n  width: 20%;\r\n  min-width: 250px;\r\n  border-left: 3px solid black;\r\n  position: absolute;\r\n  top: 0;\r\n  right: 0;\r\n  @media (max-width: 700px) {\r\n    width: 100%;\r\n    position: relative;\r\n    height: 50%;\r\n    border-left: none;\r\n    border-top: 5px solid black;\r\n    background-color: white;\r\n    margin: 0;\r\n    z-index: 1;\r\n  }\r\n  h1{\r\n    color: black;\r\n  }\r\n  button{\r\n    width: 80%;\r\n    margin: 5px auto;\r\n    font-size: 30px;\r\n  }\r\n}\r\n</style>"],"sourceRoot":""}]}